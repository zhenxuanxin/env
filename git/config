[core]
	editor = vim
	autocrlf = input
	safecrlf = true
	quotepath = false
[credential]
	helper = store
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[http]
	sslVerify = true
[http "https://github.com"]
	proxy = http://127.0.0.1:1080
[push]
	autoSetupRemote = true
	default = simple
